IF
	!See([EVILCUTOFF.UNDEAD])
	!Range([GOODCUTOFF],6)
THEN
	RESPONSE #100
		MoveToObject([GOODCUTOFF])
END

IF
	See([EVILCUTOFF.UNDEAD])
	Range([EVILCUTOFF.UNDEAD],5)
THEN
	RESPONSE #100
		Turn()
END

IF
	See([EVILCUTOFF.UNDEAD])
	!Range([EVILCUTOFF.UNDEAD],6)
THEN
	RESPONSE #100
		MoveToObject([EVILCUTOFF.UNDEAD])
		Turn()
END
